{
  "name": "bench",
  "version": "0.0.1",
  "main": "./src/electron.js",
  "description": "Your new workbench",
  "author": {
    "name": "Cosmin Serdean",
    "email": "cosmin@cserdean.com",
    "url": "https://cserdean.com"
  },
  "build": {
    "productName": "bench",
    "appId": "com.cserdean.bench",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "icons/icon.png",
      "extendInfo": {
        "LSUIElement": 1
      }
    }
